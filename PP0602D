//  2015-01-26 20:00:50

#include <stdio.h>

int main()
{
   int n;
   int k;
   int a;
   scanf("%i%i",&n,&k);
   int t[n];
   int q[k];
   for(int i=0;i<n;i++)
   {
       scanf("%i",&a);
       t[i]=a;
   }
   for(int i=0;i<k;i++)
   {
     q[i]=t[i];
   }
   for(int i=k;i<n;i++)
   {
       printf("%i ",t[i]);
   }
   for(int i=0;i<k;i++)
   {
       printf("%i ",q[i]);
   }
   return 0;
}
