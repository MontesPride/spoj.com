//  2015-01-25 22:48:42

#include <stdio.h>

int main()
{
    int x;
    int c,k;
    scanf("%i",&x);
    for(int i=0; i<x; i++)
    {
        scanf("%i %i",&k,&c);
        if(k==1)
            printf("TAK\n");
        else if(c%(k-1)==0)
            printf("NIE\n");
        else
            printf("TAK\n");
    }
    return 0;
}
