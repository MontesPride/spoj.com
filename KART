//  2015-03-08 20:09:16

#include <cstdio>

int main()
{
    int x=0;
       int t;
       int n;
       int q=5;
       scanf("%i",&t);
       for(int i=0;i<t;i++)
       {
         scanf("%i",&n);
         while(q<=n)
         {
            x=x+n/q;
            q*=5;
         }
         printf("%i\n",x);
         x=0;
         q=5;
       }
        return 0;
}
