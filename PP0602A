//  2015-01-24 23:29:12

#include <iostream>

using namespace std;

int main()
{
    int *wsk;
    int t;
    int n;
    int a;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        cin>>n;
        wsk = new int[n];
        for(int j=0;j<n;j++)
        {
            cin>>a;
            wsk[j]=a;
        }
        for(int j=1;j<n;j=j+2)
        {
            cout<<wsk[j]<<" ";
        }
        for(int j=0;j<n;j=j+2)
        {
            cout<<wsk[j]<<" ";
        }
        cout<<endl;
    }
    return 0;
}
