//  2015-03-05 20:40:32

#include <iostream>

using namespace std;

int main()
{
    int x;
    string a, b;
    bool t;
    cin>>x;
    for(int i=0;i<x;i++)
    {
        t=false;
        cin>>a>>b;
        for(int j=0;j<a.size();j++)
        {
            if(a==b)
                {
                t=true;
                break;
                }
            else
            {
                a=a+a[0],
                a.erase(a.begin()+0);
            }
        }
        if(t==true)
            cout<<"yes"<<endl,
            t=false;
        else
            cout<<"no"<<endl;
    }
    return 0;
}
