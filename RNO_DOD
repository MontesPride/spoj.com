//  2015-01-23 21:26:44

#include<iostream>
#include<cstdlib>
using namespace std;


int main()
{
    int test,liczb,a,suma=0;
    cin>>test;
    for(int i=0;i<test;i++)
    {
        cin>>liczb;
        for(int j=0;j<liczb;j++)
        {
            cin>>a;
            suma=suma+a;
        }
        cout<<suma<<endl;
        suma=0;
    }
    return 0;
}
