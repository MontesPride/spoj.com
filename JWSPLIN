//  2015-02-05 22:00:20

#include <iostream>

using namespace std;

int main()
{
    int x;
    float a,b,c,d,e,f;
    cin>>x;
    for(int i=0; i<x; i++)
    {
        cin>>a>>b>>c>>d>>e>>f;
        if(a==c && e==a)
            cout<<"TAK"<<endl;
        else
            if((a==c && e!=a) || a!=c && e==a)
            cout<<"NIE";
        else
        {
        if(((d-b)/(c-a))==((f-b)/(e-a)))
            cout<<"TAK"<<endl;
        else
            cout<<"NIE"<<endl;
        }
    }
    return 0;
}
