#include<iostream>
#include<math.h>
using namespace std;


int main()
{
    int a,b,x;
    cin>>x;
    for(int i=0;i<x;i++)
    {
    cin>>a>>b;
    if(b==0)
        cout<<"1"<<endl;
    else
    switch(a%10)
    {
    case 1:
        cout<<"1"<<endl;
        break;
    case 2:
        switch (b%4)
        {
        case 0:
            cout<<"6"<<endl;
            break;
        case 1:
            cout<<"2"<<endl;
            break;
        case 2:
            cout<<"4"<<endl;
            break;
        case 3:
            cout<<"8"<<endl;
            break;
        }
        break;
    case 3:
        switch (b%4)
        {
        case 0:
            cout<<"1"<<endl;
            break;
        case 1:
            cout<<"3"<<endl;
            break;
        case 2:
            cout<<"9"<<endl;
            break;
        case 3:
            cout<<"7"<<endl;
            break;
        }
        break;
    case 4:
        switch (b%2)
        {
        case 0:
            cout<<"6"<<endl;
            break;
        case 1:
            cout<<"4"<<endl;
            break;
        }
        break;
    case 5:
        cout<<"5"<<endl;
        break;
    case 6:
        cout<<"6"<<endl;
    case 7:
        switch (b%4)
        {
        case 0:
            cout<<"1"<<endl;
            break;
        case 1:
            cout<<"7"<<endl;
            break;
        case 2:
            cout<<"9"<<endl;
            break;
        case 3:
            cout<<"3"<<endl;
            break;
        }
        break;
        case 8:
            switch (b%4)
        {
        case 0:
            cout<<"6"<<endl;
            break;
        case 1:
            cout<<"8"<<endl;
            break;
        case 2:
            cout<<"4"<<endl;
            break;
        case 3:
            cout<<"2"<<endl;
            break;
        }
        break;
        case 9:
            switch (b%2)
        {
        case 0:
            cout<<"1"<<endl;
            break;
        case 1:
            cout<<"9"<<endl;
            break;
        }
        break;
        case 0:
            cout<<"0"<<endl;
            break;
    }
    }
    return 0;
}
