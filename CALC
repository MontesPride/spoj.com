//  2015-01-24 12:40:34

#include <iostream>

using namespace std;

int main()
{
char n;
int a,b;
while(cin>>n)
{
    if(n=='+')
    {
        cin>>a>>b;
        cout<<a+b<<endl;
    }
    if(n=='-')
    {
        cin>>a>>b;
        cout<<a-b<<endl;
    }
    if(n=='/')
    {
        cin>>a>>b;
        cout<<a/b<<endl;
    }
    if(n=='*')
    {
        cin>>a>>b;
        cout<<a*b<<endl;
    }
    if(n=='%')
    {
        cin>>a>>b;
        cout<<a%b<<endl;
    }
}
    return 0;
}
