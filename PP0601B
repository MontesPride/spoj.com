//  2015-02-05 20:25:26

#include <cstdlib>
#include <iostream>

using namespace std;

int main(int argc, char *argv[])
{
    int t,n,x,y;
    cin>>t;
    for (int i=0;i<t;i++) {
        cin>>n;
        cin>>x;
        cin>>y;

        for (int j=1;j<=n;j++) {
        if (j%x==0 && j%y!=0) cout<<j<<" ";
        }
        cout<<endl;
    }
    return 0;
}
