#include <stdio.h>

int main()
{
    int n;
    scanf("%i",&n);
    int *wsk;
    if(n==1 || n==2)
        printf("NIE");
    else
        if(n==3)
        printf("2 0 3 1");
    else
        {
            wsk = new int [n+1];
            for(int i=0;i<=n;i++)
            {
                wsk[i]=i;
            }
            for(int i=0;i<=n;i+=2)
            {
                printf("%i ",wsk[i]);
            }
            for(int i=1;i<=n;i+=2)
            {
              printf("%i ",wsk[i]);
            }
        }
        return 0;
}
