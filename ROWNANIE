//  2015-01-23 21:34:43

#include<iostream>
#include<cstdlib>
using namespace std;


int main()
{
    float a,b,c;

    while(cin>>a)
    {
        cin>>b>>c;
        if((b*b)-(4*(a*c))<0)
            cout<<"0"<<endl;
        if((b*b)-(4*(a*c))==0)
            cout<<"1"<<endl;
        if((b*b)-(4*(a*c))>0)
            cout<<"2"<<endl;
    }
    return 0;
}
