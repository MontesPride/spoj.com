// 2015-03-01 13:43:52

#include <iostream>
#include <vector>

using namespace std;

int main()
{
    vector<int> t;
    int x,n,a;
    cin>>x;
    for(int i=0;i<x;i++)
    {
        cin>>n;
        for(int j=0;j<n;j++)
        {
            cin>>a;
            t.push_back(a);
        }
        for(int j=t.size()-1;j>=0;j--)
        {
            cout<<t[j]<<" ";
        }
        cout<<endl;
        t.resize(0);
    }
    return 0;
}


